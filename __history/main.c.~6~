#include <main.h>
int8 dem=0;
#INT_TIMER0
void ngatTimer()
{
   dem++;
   if(dem>76)
   {
      output_bit(PIN_D0,0);
      delay_ms(100);
   }
}

void main()
{  
   setup_timer_0(T0_INTERNAL|T0_DIV_256);
   set_timer0(0);
   enable_interrupts(INT_TIMER0);
   enable_interrupts(GLOBAL);
   output_d(0xFF);
}
